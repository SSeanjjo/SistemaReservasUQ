<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="center" prefHeight="400.0" prefWidth="400.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.reservasuq.controlador.InicioControlador">



    <Label text="Bienvenido a la app ReservasUQ">
        <font>
            <Font size="18.0" />
        </font>
    </Label>


    <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
<!--        <Image url="@img/logo.jpg"/>-->
    </ImageView>


    <Label text="¿Qué desea hacer?">
        <font>
            <Font size="14.0" />
        </font>
    </Label>


    <HBox alignment="CENTER" spacing="20.0">
        <Button minHeight="40.0" minWidth="130.0" onAction="#irIniciarSesion" text="Iniciar sesión" fx:id="btnIniciarSesion"/>
        <Button minHeight="40.0" minWidth="130.0" onAction="#irRegistroCliente" text="Registrarse" />

    </HBox>




</VBox>
